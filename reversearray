package ArraysQues;

import java.util.Arrays;

public class ReverseArray {
	public static void main() {
		int[] arr= {1,5,9,2,3,7};
		int n=arr.length;
		for(int i=0,j=n-1;i<j&&i<n&&j>0;i++,j--) {
			int temp=arr[i];
			arr[i]=arr[j];
			arr[j]=temp;
		
	}
		System.out.println(Arrays.toString(arr));
	}
	
}
