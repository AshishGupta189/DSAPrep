import java.util.*;


class Main{
    public static void main(String[] arga){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        for(int i=0;i<n;i++){
            int s=sc.nextInt();
            String str=sc.next();
            for(int j=0;j<s;j++){
                String op="";
                for(int k=j;k<s;k++){
                    op+=str.charAt(k);
                    System.out.println(op);
                }
            }
        }
    }
}