static ListNode deleNodeAtStart(ListNode ln){

            head.data=head.next.data;
            head.next=head.next.next;

            return head;
        }

static ListNode deleNodeFromLast(ListNode ln){

            ListNode ll=ln;
            while(ll.next.next!=null){
                ll=ll.next;
            }
            ll.next=null;

            return ll;
        }
